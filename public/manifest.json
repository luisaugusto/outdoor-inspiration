{
  "name": "Outdoor Inspiration",
  "description": "A Chrome tab extension that displays pictures and descriptions of US National Parks",
  "version": "1.0.0",
  "content_security_policy": "script-src 'self' https://kit.fontawesome.com",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "manifest_version": 2
}